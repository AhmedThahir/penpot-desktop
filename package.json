  "main": "src/process/index.js",
  "homepage": "https://sudovanilla.com/code/Korbs/Penpot-Desktop/",
  "funding": [
    {
      "type": "individual",
      "url": "https://ko-fi.com/korbsstudio"
    },
    {
      "type": "individual",
      "url": "https://sudovanilla.com/donate/"
    }
  ],
  "scripts": {
    "start": "npm run dev",
    "build": "electron-builder --config build/electron-builder.yml",
    "build:win": "electron-builder --config build/electron-builder.yml --win",
    "build:dmg": "sh ./build/build-dmg.sh",
    "dev": "concurrently --kill-others \"npm run scss:app\" \"electron .\"",
    "scss:app": "stylus --watch src/base/frontend/stylesheets/index.styl src/base/frontend/stylesheets/index.css"
  },
  "dependencies": {
    "concurrently": "^7.6.0",
    "electron-is-dev": "^2.0.0",
    "electron-re": "^1.3.0",
    "electron-tabs": "^1.0.1",
    "electron-titlebar-respect": "^1.0.0",
    "electron-updater": "^5.3.0",
    "stylus": "^0.59.0"
  },
  "devDependencies": {
    "electron": "^23.2.0",
    "electron-builder": "^22.10.3",
    "electron-installer-dmg": "^4.0.0"
  }
}
