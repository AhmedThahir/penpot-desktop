  "main": "src/process/index.js",
  "homepage": "https://sudovanilla.com/code/Korbs/Penpot-Desktop/",
  "funding": [
    {
      "type": "individual",
      "url": "https://ko-fi.com/korbsstudio"
    }
  ],
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --config build/electron-builder.yml",
    "build:win": "electron-builder --config build/electron-builder.yml --win",
    "dev": "concurrently --kill-others \"npm run scss:app\" \"npm start\"",
    "scss:app": "sass --watch src/base/frontend/stylesheets/index.scss:src/base/frontend/stylesheets/index.css"
  },
  "dependencies": {
    "concurrently": "^7.6.0",
    "electron-is-dev": "^2.0.0",
    "electron-tabs": "^1.0.1",
    "electron-titlebar-respect": "^1.0.0",
    "electron-updater": "^5.3.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "electron": "^23.1.1",
    "electron-builder": "^22.10.3"
  }
}
